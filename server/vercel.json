{
   "version": 2,
   "builds": [
      {
         "src": "/src/app.js",
         "use": "@vercel/node",
         "config": {
            "includeFiles": [
               "dist/**"
            ]
         }
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "/src/app.js"
      }
   ]
}